@article{norin2023109020,
title = {Breaking the degeneracy barrier for coloring graphs with no Kt minor},
journal = {Advances in Mathematics},
volume = {422},
pages = {109020},
year = {2023},
issn = {0001-8708},
doi = {https://doi.org/10.1016/j.aim.2023.109020},
url = {https://www.sciencedirect.com/science/article/pii/S0001870823001639},
author = {Sergey Norin and Luke Postle and Zi-Xia Song},
keywords = {Graph theory, Graph coloring, Graph minors, Hadwiger's conjecture},
abstract = {In 1943, Hadwiger conjectured that every graph with no Kt minor is (t−1)-colorable for every t≥1. In the 1980s, Kostochka and Thomason independently proved that every graph with no Kt minor has average degree O(tlog⁡t) and hence is O(tlog⁡t)-colorable. We show that every graph with no Kt minor is O(t(log⁡t)β)-colorable for every β>1/4, making the first improvement on the order of magnitude of the Kostochka-Thomason bound.}
}

